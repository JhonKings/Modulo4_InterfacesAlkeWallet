<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentSplashScreen_1">
    <fragment
        android:id="@+id/fragmentSplashScreen_1"
        android:name="com.example.wallet.fragments.FragmentSplashScreen_1"
        android:label="fragment_splash_screen_1"
        tools:layout="@layout/fragment_splash_screen_1" >
        <action
            android:id="@+id/action_fragmentSplashScreen_1_to_fragmentLoginSingupPage_2"
            app:destination="@id/fragmentLoginSingupPage_2" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLoginSingupPage_2"
        android:name="com.example.wallet.fragments.FragmentLoginSingupPage_2"
        android:label="fragment_login_singup_page_2"
        tools:layout="@layout/fragment_login_singup_page_2" >
        <action
            android:id="@+id/action_fragmentLoginSingupPage_2_to_fragmentLoginPage_3"
            app:destination="@id/fragmentLoginPage_3" />
        <action
            android:id="@+id/action_fragmentLoginSingupPage_2_to_fragmentSplashScreen_1"
            app:destination="@id/fragmentSplashScreen_1" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLoginPage_3"
        android:name="com.example.wallet.fragments.FragmentLoginPage_3"
        android:label="fragment_login_page_3"
        tools:layout="@layout/fragment_login_page_3" >
        <action
            android:id="@+id/action_fragmentLoginPage_3_to_fragmentSingupPage_4"
            app:destination="@id/fragmentSingupPage_4" />
        <action
            android:id="@+id/action_fragmentLoginPage_3_to_fragmentLoginSingupPage_2"
            app:destination="@id/fragmentLoginSingupPage_2" />
    </fragment>
    <fragment
        android:id="@+id/fragmentSingupPage_4"
        android:name="com.example.wallet.fragments.FragmentSingupPage_4"
        android:label="fragment_singup_page_4"
        tools:layout="@layout/fragment_singup_page_4" >
        <action
            android:id="@+id/action_fragmentSingupPage_4_to_fragmentLoginPage_3"
            app:destination="@id/fragmentLoginPage_3" />
    </fragment>
    <fragment android:id="@+id/placeholder" />
</navigation>